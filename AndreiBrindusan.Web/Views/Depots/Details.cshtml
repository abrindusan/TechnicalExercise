@model AndreiBrindusan.DataModel.Depot

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Depot</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DepotName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DepotName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DepotId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DepotId)
        </dd>
        <dt class="col-sm-2">
            Countries
        </dt>
        <dd class="col-sm-10">
            @foreach (var country in Model.Countries)
            {
                <div>@country.CountryName</div>
            }
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.DepotId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>

